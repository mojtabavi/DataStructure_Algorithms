   "source": [
    "def insertion_sort(arr):\n",
    "    n = len(arr)\n",
    "    for item in range(1,n):\n",
    "        key = arr[item]\n",
    "        j = item - 1\n",
    "        while j >= 0 and arr[j] >= key:\n",
    "            arr[j + 1] = arr[j]\n",
    "            j -= 1\n",
    "        arr[j + 1] = key\n",
    "    \n",
    "    return arr"
   ]
  },
