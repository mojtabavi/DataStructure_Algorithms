    "A = [ 5, 12, 3, 4, 7, 1, 0, 6, 19, 8, 13]\n",
    "n = len(A)\n",
    "for i in range(n-1):\n",
    "    bubble_found = False\n",
    "    for j in range(n-1,i,-1):\n",
    "        if A[j] < A[j-1]:\n",
    "            A[j],A[j-1] = A[j-1],A[j]\n",
    "            bubble_found = True\n",
    "    if not bubble_found:\n",
    "        break\n",
    "    print(A,i)"
